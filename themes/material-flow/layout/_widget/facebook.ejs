<% if(config.facebook_fans.enable) { %>
<section class="m_widget">
    <div class="fb-page" 
        data-href="<%= config.facebook_fans.url %>" 
        data-tabs="timeline" 
        data-width="250" 
        data-small-header="false" 
        data-adapt-container-width="true" 
        data-hide-cover="false" 
        data-show-facepile="true">
        <blockquote cite="<%= config.facebook_fans.url %>" class="fb-xfbml-parse-ignore">
            <a href="<%= config.facebook_fans.url %>"><%= config.facebook_fans.name %></a>
        </blockquote>
    </div>
</section>
<% } %>